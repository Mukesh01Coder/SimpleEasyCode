#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
int main(){
     ios::sync_with_stdio(0);
     cin.tie(0);
     ll t; cin>>t;
    
     while(t--){
          ll n; cin >> n;
        
           ll count = 0;
         
          for(ll i=5;(n/i)>0;i = i*5){
               count = count + n/i;
          }
          cout<<count<<'\n';
     }
}
